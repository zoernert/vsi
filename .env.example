# Server Configuration
NODE_ENV=development
PORT=3000

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=vsi_vector_store
DB_USER=postgres
DB_PASSWORD=password

# Qdrant Vector Database
QDRANT_URL=http://localhost:6333

# Google AI Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-1.5-flash
EMBEDDING_MODEL=text-embedding-004

# Note: Google text-embedding-004 produces 768-dimensional vectors
# This is different from OpenAI's text-embedding-3-small (1536 dimensions)

# Authentication
JWT_SECRET=your_super_secret_jwt_key_change_in_production
JWT_EXPIRES_IN=24h

# Default Admin User
ADMIN_USERNAME=stromdao
ADMIN_PASSWORD=Maus12Rad

# File Upload
MAX_FILE_SIZE=50MB
UPLOAD_PATH=./uploads

# Rate Limiting
RATE_LIMIT_AUTH=5
RATE_LIMIT_SEARCH=20
RATE_LIMIT_GENERAL=100

# Logging
LOG_LEVEL=info
LOG_FILE=logs/app.log
