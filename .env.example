# Database
QDRANT_URL=http://localhost:6333

# Authentication
JWT_SECRET=your-secret-key-here
# API_KEY=your-api-key-here  # Optional: for API key authentication

# User Registration Control
ALLOW_SELF_REGISTRATION=false  # Set to true to allow user self-registration
ALLOW_RAPIDAPI_USERS=false     # Set to true to allow RapidAPI users without registration

# Admin Configuration
ADMIN_USERNAME=admin           # Default admin username
ADMIN_PASSWORD=admin123        # Default admin password (change this!)

# Google AI (required for embeddings)
GOOGLE_AI_API_KEY=your-google-ai-key-here

# Server
PORT=3000
BASE_URL=http://localhost:3000  # Base URL for API examples (auto-detected if not set)

# MCP Configuration (optional)
MCP_LOG_LEVEL=info
